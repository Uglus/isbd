        <StackPanel Margin="40,34,40,40">
            <Border Background="#ffcf40" Margin="10, 20" CornerRadius="10" Height="495">
                <ListView>
                    <ListView.View>
                        <GridView>
                            <GridViewColumn Header="PLAYERS" />
                            <GridViewColumn Header="PONTS" />
